  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/app",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TS,Node\" -c \"cyan,green\" \"npm run watch:ts\" \"npm run watch:node\"",
